{
  "name": "aipage-widget",
  "version": "1.0.5",
  "description": "aipage-editor自定义组件注册器（支持react和vue2.0技术栈），用于注册自定义渲染器和插件",
  "keywords": [
    "aipage-widget",
    "自定义组件注册器",
    "aipage-editor自定义组件",
    "aipage-editor自定义插件"
  ],
  "author": "wibetter",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "amis dev",
    "preview": "amis preview",
    "linkDebug": "amis linkDebug",
    "build2lib": "amis build2lib",
    "build2esm": "amis build2esm",
    "format": "prettier --write \"{src,demo}/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\""
  },
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.umd.min.js",
        "default": "./dist/index.esm.min.js"
      },
      "require": "./dist/index.umd.min.js",
      "types": "./types/main.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://git@github.com:aisuda/aipage-widget.git"
  },
  "bugs": {
    "url": "https://github.com/aisuda/aipage-widget/issues"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/lodash": "^4.14.182",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^17.0.13",
    "amis-widget-cli": "^2.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "react-dom": "^16.8.6"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  }
}
